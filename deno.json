{
  "name": "retrieval-format-bench",
  "version": "0.3.0",
  "nodeModulesDir": "none",
  "imports": {
    "@std/cli": "jsr:@std/cli@^1.0.27",
    "@std/fs": "jsr:@std/fs@^1.0.22",
    "@std/path": "jsr:@std/path@^1.1.4",
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.6",
    "@std/yaml": "jsr:@std/yaml@^1.0.11",
    "@std/csv": "jsr:@std/csv@^1.0.6",
    "@libs/xml": "jsr:@libs/xml@^7.0.3"
  },
  "tasks": {
    "bench": "deno run --allow-env --allow-net --allow-read --allow-write src/cli.ts bench",
    "report": "deno run --allow-read --allow-write src/cli.ts report",
    "report:uniform": "deno run --allow-read --allow-write tasks/uniform_report.ts",
    "models:refresh": "deno run --allow-env --allow-net --allow-write src/cli.ts models:refresh",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "estimate": "deno run --allow-env --allow-net --allow-read src/cli.ts estimate"
  },
  "fmt": {
    "lineWidth": 100,
    "semiColons": true,
    "singleQuote": false
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "exports": "./src/mod.ts"
}
